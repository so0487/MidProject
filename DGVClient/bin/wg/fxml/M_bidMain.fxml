<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane prefHeight="650.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="wg.main.M_bidController">
   <center>
      <ScrollPane prefWidth="960.0" style="-fx-border-color: #c7c9c8;" BorderPane.alignment="CENTER">
         <content>
            <TableView fx:id="table" onMouseClicked="#click" prefHeight="620.0" prefWidth="960.0" style="-fx-border-color: white;">
               <columns>
                  <TableColumn fx:id="bidid" prefWidth="100.0" style="-fx-alignment: center;" text="낙찰ID" />
                  <TableColumn fx:id="memid" prefWidth="180.0" style="-fx-alignment: center;" text="회원ID" />
                  <TableColumn fx:id="aucno" prefWidth="253.0" style="-fx-alignment: center;" text="경매 번호" />
                  <TableColumn fx:id="bidtime" prefWidth="206.0" style="-fx-alignment: center;" text="낙찰 시간" />
                  <TableColumn fx:id="bidprice" prefWidth="215.0" style="-fx-alignment: center;" text="낙찰가" />
               </columns>
            </TableView>
         </content>
      </ScrollPane>
   </center>
   <bottom>
      <HBox prefHeight="3.0" prefWidth="960.0" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets top="10.0" />
         </BorderPane.margin>
      </HBox>
   </bottom>
   <top>
      <AnchorPane prefHeight="14.0" prefWidth="960.0" style="-fx-border-color: #c7c9c8;" BorderPane.alignment="CENTER">
         <padding>
            <Insets bottom="20.0" />
         </padding>
      </AnchorPane>
   </top>
</BorderPane>
